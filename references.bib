@inproceedings{Xu2011,
	title        = {Magnetocaloric effect in ErSi compound},
	author       = {Z. Y. Xu and J. Shen and X. Q. Zheng and H. Zhang},
	year         = 2011,
	month        = 10,
	journal      = {IEEE Transactions on Magnetics},
	volume       = 47,
	pages        = {2470--2473},
	doi          = {10.1109/TMAG.2011.2153837},
	issn         = 00189464,
	abstract     = {The magnetic properties and magnetocaloric effects MCEs have been investigated in ErSi compound which crystallizes in the orthorhombic CrB-type structure. This compound is antiferromagnetic AFM with a N el temperature TN of 12.5 K. The measurements of the temperature and field dependences of the magnetization clearly show that the ErSi compound undergoes a field-induced metamagnetic transition from AFM to ferromagnetic FM state below TN. The isothermal magnetic entropy change SM and refrigerant capacity RC of ErSi have been measured for various applied magnetic fields and their values are found to be -14.8J kg-1 K-1 and 210 J kg -1 for a field change of 0-5 T respectively. The large MCE is attributed to the field-induced AFM to FM metamagnetism occurring in this compound. 2011 IEEE.},
	issue        = 10,
	keywords     = {Antiferromagnetic AFM materials magnetic refrigeration magnetocaloric effect MCE}
}
@article{Xu2011-11,
	title        = {Inorganic Materials Database for Exploring the Nature of Material},
	author       = {Yibin Xu and Masayoshi Yamazaki and Pierre Villars},
	year         = 2011,
	month        = 11,
	journal      = {Japanese Journal of Applied Physics},

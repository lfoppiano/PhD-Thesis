}
@article{Li2013,
	title        = {Two successive magnetic transitions induced large refrigerant capacity in HoPdIn compound},
	author       = {Lingwei Li and Takahiro Namiki and Dexuan Huo and Zhenghong Qian and Katsuhiko Nishimura},

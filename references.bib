@article{Hu2000,
	title        = {Great magnetic entropy change in La Fe M 13 M Si Al with Co doping},
	author       = {Feng Xia Hu and Bao Gen Shen and Ji Rong Sun and Xi Xiang Zhang},
	year         = 2000,
	month        = 7,
	journal      = {Chinese Physics},
	volume       = 9,
	pages        = {550--553},
	doi          = {10.1088/1009-1963/9/7/016},
	issn         = 10091963,
	url          = {http://stacks.iop.org/1009-1963/9/i 7/a 016 key crossref.191c855afd370d2c8fd71ff8fef8cd23},
	abstract     = {Very large magnetic entropy change S M which originates from a fully reversible second-order transition at Curie temperature T C has been discovered in compounds La Fe A1 13 La Fe Al 13 and those with Co doping. The maximum change S M 19J kg -1 K -1 achieved in LaFe 11.4 Si 1.6 at 209K upon a 5T magnetic field change exceeds that of Gd by more than a factor of 2. The T C of the Co-doped compounds shifts to higher temperatures. S M still has a considerable large magnitude near room temperature. The phenomena of very large S M convenience of adjustment of TC and also the superiority of low cost strongly suggest that the compounds La Fe M 13 M Si Al with Co doping are suitable candidates for magnetic refrigerants at high temperatures.},
	issue        = 7,
	keywords     = {Magnetic entropy change NaZn13-type compounds}
}
@article{Hu2001,
	title        = {Influence of negative lattice expansion and metamagnetic transition on magnetic entropy change in the compound LaFe11.4Si1.6},
	author       = {Feng Xia Hu and Bao Gen Shen and Ji Rong Sun and Zhao Hua Cheng and Guang Hui Rao and Xi Xiang Zhang},
	year         = 2001,
	journal      = {Applied Physics Letters},
	volume       = 78,
	pages        = {3675--3677},
	doi          = {10.1063/1.1375836},
	issn         = 00036951,
	abstract     = {Magnetization of the compound LaFe11.4Si1.6 with the cubic NaZn13-type structure was measured as functions of temperature and magnetic field around its Curie temperature TC of 208 K. It is found that the magnetic phase transition at TC is completely reversible. Magnetic entropy change S allowing one to estimate the magnetocaloric effect was determined based on the thermodynamic Maxwell relation. The achieved magnitude of S reaches 19.4 J/kg K under a field of 5 T which exceeds that of most other materials involving a reversible magnetic transition in the corresponding temperature range. The large entropy change is ascribed to the sharp change of magnetization which is caused by a large negative lattice expansion at the TC. An asymmetrical broadening of S peak with increasing field was observed which is resulted from the field-induced itinerant-electron metamagnetic transition from the paramagnetic to ferromagnetic state above the TC. 2001 American Institute of Physics. 2001 American Institute of Physics},
	issue        = 23
}

@article{Wu2017,
	title        = {Phase transition-induced magnetocaloric effects in R2Mo2O7 R Er Dy Gd and Y molybdates},
	author       = {Yao Dong Wu and Qiao Yan Dong and Yue Ma and Ya Jiao Ke and Na Su and Xiang Qun Zhang and Li Chen Wang and Zhao Hua Cheng},
	year         = 2017,
	month        = 7,
	journal      = {Materials Letters},
	publisher    = {Elsevier B.V.},
	volume       = 198,
	pages        = {110--113},
	doi          = {10.1016/j.matlet.2017.04.007},
	issn         = 18734979,
	abstract     = {We have investigated the magnetic properties and magnetocaloric effects MCEs of R2Mo2O7 R Er Dy Gd and Y . Magnetic ground states of R2Mo2O7 largely depend on the radius of rare earth ion. Er2Mo2O7 Dy2Mo2O7 and Y2Mo2O7 with small rare earth radius exhibit spin-glass state around 20 K while Gd2Mo2O7 with larger rare earth radius undergoes a ferromagnetic state below 78 K and a reentrant spin-glass behavior below 34 K. Magnetic phase transition leads to magnetocaloric effect. In particular a natural table-like - SM and a very large refrigerant capacity RC value of 94.4 J/kg was observed in a field change of 0 10.5 kOe for Gd2Mo2O7 due to two successive magnetic transitions at 78 and 10 K. These notable magnetocaloric properties make the Gd2Mo2O7 a potential candidate for low-field Ericsson-cycle magnetic refrigeration in the low temperature range.},
	keywords     = {Magnetic material Phase transformation}
}
@article{Wu2019,
	title        = {Magnetic properties and large magnetocaloric effect in Ho 3 Pd 2 compound},
	author       = {X. F. Wu and C. Guo and J. Wang and J. Q. Deng and Y. S. Du and G. H. Rao and Z. Du},
	year         = 2019,
	month        = 7,
	journal      = {Journal of Magnetism and Magnetic Materials},
	publisher    = {Elsevier B.V.},
	volume       = 482,
	pages        = {168--172},
	doi          = {10.1016/j.jmmm.2019.03.048},
	issn         = 03048853,
	abstract     = {Magnetic transitional behaviors and magnetocaloric effect of the intermetallic compound Ho 3 Pd 2 have been studied. It exhibits a second order magnetic transition from ferromagnetic FM to paramagnetic PM states at a Curie temperature T C of 9.6 K. In the PM region the magnetic susceptibility of the Ho 3 Pd 2 compound follows the Curie-Weiss law. The paramagnetic Curie temperature p was evaluated to be 5.3 K and the effective magnetic moment eff to be 10.5 B /Ho. The positive value of p indicates the ferromagnetic interactions in the ordering state of Ho 3 Pd 2 and the effective magnetic moment is close to the theoretical value 10.6 B for the Ho 3 ion which suggests that the Pd ions in Ho 3 Pd 2 are non-magnetic. Under the magnetic field change of 0 5 T the maximum magnetic entropy change S M and the refrigerant capacity RC for this compound are evaluated to be 18.3 J kg 1 K 1 and 301.5 J kg 1 respectively with no thermal and field hysteresis loss.},
	keywords     = {Magnetization Magnetocaloric effect Rare earth compound}
}

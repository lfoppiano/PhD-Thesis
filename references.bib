@article{wei2020-ho-b-phasediagram,
	title        = {Thermodynamic assessment of the RE-B (RE=Ho, Er, Tm) binary systems},
	author       = {Q. Wei and M.H. Rong and S. Li and D. Su and Q.R. Yao and J. Wang and G.H. Rao and H.Y. Zhou},
	year         = 2020,

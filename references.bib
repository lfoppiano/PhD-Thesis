@article{Novikov2010,
	title        = {Anomalies in thermal expansion of rare-earth diborides in the temperature range of magnetic phase transformations},
	author       = {V. V. Novikov and T. A. Chukina and A. A. Verevkin},
	year         = 2010,
	month        = 2,
	journal      = {Physics of the Solid State},
	volume       = 52,
	pages        = {364--369},
	doi          = {10.1134/S106378341002023X},
	isbn         = 1063783410,
	issn         = {1063-7834},
	abstract     = {The variations with temperature of the lattice parameters a T and c T and the thermal expansion coefficients of rare-earth diborides RB2 R Tb Dy Ho Er Tm Lu have been investigated in the temperature range 4. 2-300 K. It has been revealed that the dependences a T and c T exhibit an anomalous behavior in the vicinity of the ferromagnetic transformation temperatures Tc which correlates with the temperature dependences of the heat capacity. It has been established that the magnetic contributions to the thermal expansion of the diborides am T increase with increasing temperature and tend to constant values at T Tc and that the magnetic components cm T decrease for all the diborides studied also tending to constant values at elevated temperatures. The exchange integrals Ya and Yc for the paramagnetic metal ions in the rare-earth diborides are determined from the dependences am T and cm T . 2010 Pleiades Publishing Ltd.},
	issue        = 2
}
@article{Novikov2017,
	title        = {Synthesis thermal and magnetic properties of RE-diborides},

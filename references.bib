@article{Wang2006,
	title        = {Structure and magnetic properties of RNi sub 2 /sub Mn compounds R Tb Dy Ho and Er},
	author       = {J. L. Wang and C. Marquina and M. R. Ibarra and G. H. Wu},
	year         = 2006,

@article{Zhu2011,
	title        = {Magnetocaloric effect of Er sub x /sub R sub 1-x /sub Co sub 2 /sub R Ho Dy for magnetic refrigeration between 20 and 80 K},
	author       = {Yiyin Zhu and Kai Asamoto and Yuta Nishimura and Takaaki Kouen and Satoshi Abe and Koichi Matsumoto and Takenori Numazawa},
	year         = 2011,
	month        = 9,
	journal      = {Cryogenics},
	volume       = 51,
	pages        = {494--498},
	doi          = {10.1016/j.cryogenics.2011.06.004},
	issn         = 00112275,
	abstract     = {Two series of RCo2 compounds ErxHo 1-x Co2 and EryDy1-y Co 2 were investigated as magnetic refrigerants for hydrogen liquefaction. A large magnetocaloric effect MCE was observed just above the Curie temperature Tc of these compounds which both undergo a first-order magnetic transition. The isothermal entropy change S and the adiabatic temperature change Tad of these compounds were larger than those of other Laves-phase materials such as RAl2 and RNi2. However the temperature range of the large MCE for these compounds was limited. It has been shown that Tc increases almost linearly against the de Gennes factor and can be controlled by changing the concentration of the rare earth elements while maintaining a first-order transition. By measuring the magnetization and heat capacity we obtained temperature entropy T-S diagrams which are essential for analyzing the magnetic refrigeration cycle. Both series of compounds showed high potential for use in a regenerative thermal cycle especially as a combination of several compositions to cover a wide temperature range. 2011 Elsevier Ltd. All rights reserved.},
	issue        = 9,
	keywords     = {A. Magnetocaloric effect B. Hydrogen C. Heat capacity D. Magnetic measurements}
}
@inproceedings{Zhuo2003,
	title        = {Magnetic Entropy Change in the Ge-Rich Alloys Gd-Si-Ge},
	author       = {Yi Zhuo and R. Chahine and T. K. Bose},
	year         = 2003,
	month        = 9,
	journal      = {IEEE Transactions on Magnetics},
	volume       = 39,
	pages        = {3358--3360},
	doi          = {10.1109/TMAG.2003.816256},
	issn         = 00189464,
	abstract     = {We have studied the temperature and field dependence of the magnetocaloric effect in the Ge-rich alloys Gd5 SixGe1-x 4 where x 0.0825 0.15 and 0.1875. The low field susceptibility shows a two-step magnetic ordering at the Curie temperature TC and the Neel temperature TN. The TC of the different compositions covers the temperature range 67 K-108 K while TN remains constant at around 133 K. The magnetic entropy change Delta SM T as a function of applied field up to 7 Tesla is characterized by a second-order magnetic phase transition around TN and a first-order transition around TC. The maximum magnetic entropy change around TN is only about 3 J middot kg-1 middot K-1 but it is much larger around TC. It rapidly reaches a saturation plateau of about 40-50 J middot kg-1 middot K-1 which widens with increasing field. The width of the plateau at an applied field of 7 T is about 20 K. These results indicate that the Ge-rich alloys are potentially good candidates for MR applications at low temperature.},
	issue        = {5 II},
	keywords     = {Intermetallic compounds Magnetic entropy change Magnetic phase transition Magnetocaloric effect}
}

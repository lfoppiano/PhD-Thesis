@article{Dey2017,
	title        = {Cryogenic magnetocaloric effect in zircon-type RVO4 R Gd Ho Er and Yb},
	author       = {Koushik Dey and Ankita Indra and Subham Majumdar and Saurav Giri},
	year         = 2017,
	journal      = {Journal of Materials Chemistry C},
	volume       = 5,
	pages        = {1646--1650},
	doi          = {10.1039/c6tc05182k},
	issn         = 20507526,
	abstract     = {The demagnetization 50 kOe at liquid hydrogen temperature leads to decrease of temperature down to 3.5 K for GdVO 4 . Low-temperature magnetic refrigeration has attracted the attention of the cryogenics research community in the areas of technology in space science and in the liquefaction of hydrogen or other fuel gases although it has been less researched compared to room-temperature magnetic refrigeration. Here we report that the RVO 4 R Gd Ho Er and Yb series is promising for low-temperature cryogenic refrigeration. GdVO 4 is the most promising material exhibiting a maximum magnetocaloric effect S maxM of 41.1 J kg 1 K 1 at 3 K for a change in field H of 50 kOe. The values of S maxM are also promising with 7.94 J kg 1 K 1 at 15 K 19.7 J kg 1 K 1 at 2 K and 17.4 J kg 1 K 1 at 2 K with H 50 kOe for HoVO 4 ErVO 4 and YbVO 4 respectively. The maximum value of adiabatic temperature change T maxad attributed to the large magnetocaloric effect is 18 K for GdVO 4 with H 50 kOe. The results further indicate that demagnetization with 50 kOe field close to the liquid hydrogen temperature at 20.3 K leads to the remarkable decrease of temperature to 3.5 K. Satisfying its potential as a refrigerant material the thermal hysteresis of magnetization is negligible and the values of S maxM and T maxad are as high as 12.4 J kg 1 K 1 and 8.5 K for 20 kOe change in field which can be achieved without a superconducting magnet.},
	issue        = 7
}

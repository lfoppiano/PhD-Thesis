@article{Yi2017,
	title        = {Large magnetocaloric effect in a wide temperature range induced by two successive magnetic phase transitions in Ho sub 2 /sub Cu sub 2 /sub Cd compound},
	author       = {Yalin Yi and Lingwei Li and Kunpeng Su and Yang Qi and Dexuan Huo},
	year         = 2017,
